{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "lozad": "readonly",
        "t": "readonly",
        "FrDialogs": "readonly",
        "_": "readonly",
        "AutoComplete": "readonly",
        "OC": "readonly",
        "Handlebars": "readonly",
        "n": "readonly",
        "Egg": "readonly",
        "OCA": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "vue/require-v-for-key": "off"
    }
}